{
  "name": "dnpk",
  "version": "0.0.1",
  "description": "modifiable turn based game + game engine in the spirit of the original Warlords by SSG",
  "private": "true",
  "main": "src/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "flow": "flow `# flow does not respect NODE_PATH, path alias handled in .flowconfig`",
    "l10n": "i18next 'src/core/**/*.{js,jsx}' -c src/core/l10n/i18next-parser.config.js",
    "lint": "eslint \"src/**/*.js?(x)\"",
    "start": "NODE_PATH=src/core:src/testing-grounds babel-node src/testing-grounds",
    "test": "NODE_PATH=src/core:src/testing-grounds jest --config .jest.config.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run flow && npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremyosborne/dnpk.git"
  },
  "author": "Jeremy Osborne <jeremyosborne@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jeremyosborne/dnpk/issues"
  },
  "homepage": "https://github.com/jeremyosborne/dnpk#readme",
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-flow": "^7.7.4",
    "acorn-stage3": "^2.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.0",
    "flow-bin": "^0.113.0",
    "husky": "^4.0.0-beta.1",
    "i18next-parser": "^1.0.0-beta9",
    "jest": "^24.9.0"
  },
  "dependencies": {
    "acorn": "^7.1.0",
    "ajv": "^6.10.2",
    "chalk": "^3.0.0",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "enquirer": "^2.3.2",
    "env-paths": "^2.2.0",
    "i18next": "^19.0.1",
    "lodash": "^4.17.15",
    "make-dir": "^3.0.0",
    "sprintf-js": "^1.1.2",
    "uuid": "^3.3.2"
  }
}
